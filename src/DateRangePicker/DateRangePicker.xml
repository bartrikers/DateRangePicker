<?xml version="1.0" encoding="utf-8" ?>
<widget id="DateRangePicker.widget.DateRangePicker" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
  <name>Date Range Picker</name>
  <description>A Date Range Picker, based on the http://www.daterangepicker.com plugin</description>
  <icon></icon>
  <properties>
		<!-- General -->
        <property key="startDateAttribute" type="attribute">
            <caption>Begin date (Attribute)</caption>
            <category>General</category>
			<description></description>
            <attributeTypes>
                <attributeType name="DateTime"/>
            </attributeTypes>
        </property>
		<property key="endDateAttribute" type="attribute">
            <caption>End date (Attribute)</caption>
            <category>General</category>
			<description></description>
            <attributeTypes>
                <attributeType name="DateTime"/>
            </attributeTypes>
        </property>
		<property key="labelCaption" type="translatableString" required="false">
			<caption>Label</caption>
			<category>Display</category>
			<description>The label to show on the form, leave empty to show no label.</description>
		</property>
		<property key="labelWidth" type="integer" required="true" defaultValue="3">
			<caption>LabelWidth</caption>
			<category>Display</category>
			<description>The width of the label, only applicable for horizontal display.</description>
		</property>
		<property key="displayEnum" type="enumeration" defaultValue="horizontal" required="true">
			<caption>LabelDisplay</caption>
			<category>Display</category>
			<description>How to display the label</description>
			<enumerationValues>
				<enumerationValue key="horizontal">Horizontal</enumerationValue>
				<enumerationValue key="vertical">Vertical</enumerationValue>
			</enumerationValues>
		</property>
		<property key="placeholderText" type="string" required="false">
            <caption>Placeholder text</caption>
            <category>Display</category>
            <description></description>
        </property>
        <property key="iconTooltip" type="string" required="false">
            <caption>Icon tooltip</caption>
            <category>Display</category>
            <description>Tooltip on the calendar icon.</description>
        </property>
		<!-- Date Picker options -->
		<property key="useCustomDateFormat" type="boolean" defaultValue="false">
            <caption>Use custom date format</caption>
            <category>Date Picker options</category>
            <description></description>
        </property>
		<property key="customDateFormat" type="string" defaultValue="MM/DD/YYYY" required="false">
            <caption>Custom Date format</caption>
            <category>Date Picker options</category>
            <description>Custom date format that is used, e.g. MM/DD/YYYY or DD-MM-YYYY. If timepicker is enabled, the time format can also be included, e.g. MM/DD/YYYY h:mm A or DD-MM-YYYY HH:mm</description>
        </property>
		<property key="showTimePicker" type="boolean" defaultValue="false">
            <caption>Show Timepicker</caption>
            <category>Date Picker options</category>
            <description>Allow selection of dates with times, not just dates. If you want the time to be shown, a custom date format is required.</description>
        </property>
		<property key="showWeekNumbers" type="boolean" defaultValue="false">
            <caption>Show weeknumbers</caption>
            <category>Date Picker options</category>
            <description>Show localized week numbers at the start of each week on the calendars</description>
        </property>
		<property key="show24HourTimePicker" type="boolean" defaultValue="false">
            <caption>Show 24-hour timepicker</caption>
            <category>Date Picker options</category>
            <description>Use 24-hour time picker instead of 12-hour times, removing the AM/PM selection</description>
        </property>
		<property key="timePickerIncrement" type="integer" defaultValue="1">
            <caption>Timepicker increment</caption>
            <category>Date Picker options</category>
            <description>Increment of the minutes selection list for times (i.e. 30 to allow only selection of times ending in 0 or 30)</description>
        </property>
        <!-- Behavior options -->
        <property key="onChangeMicroflow" type="microflow" required="false">
            <caption>On change microflow</caption>
            <category>Behavior</category>
            <description></description>
            <returnType type="Void" />
        </property>
		<property key="onChangeMicroflowProgress" type="boolean" defaultValue="false">
            <caption>Show progress bar</caption>
            <category>Behavior</category>
            <description></description>
        </property>
		<property key="onChangeMicroflowProgressMsg" type="string" required="false">
            <caption>Progress message</caption>
            <category>Behavior</category>
            <description></description>
        </property>
		<property key="onChangeMicroflowAsync" type="boolean" defaultValue="false">
            <caption>Asynchronous</caption>
            <category>Behavior</category>
            <description>Microflow call type.</description>
        </property>
	</properties>
</widget>